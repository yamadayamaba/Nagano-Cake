<h1>注文履歴一覧</h1>
<table>
  <thead>
    <tr>
      <th>注文日</th>
      <th>配送先</th>
      <th>支払金額</th>
    </tr>
  </thead>
  <tbody>
    <% @order.each do |order| %>
    <tr>
      <td><%= link_to created_at.strftime('%Y/%m/%d') %></td>
      <td><%= order.address %></td>
      <td><%= @cart_item.subtotal %></td>
    </tr>
    <% end %>
  </tbody>
</table>

